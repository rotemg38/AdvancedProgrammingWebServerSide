@model Models.Rate

@{
	ViewData["Title"] = "Create";
}
<link rel="stylesheet" href="~/css/create.css" />
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

<hr />
<div>
	<div className="row align-items-center justify-content-center card">
		<div className="form col-lg-5 ">
			<div className="headline">
				<h1>Create</h1>
			</div>
			<form asp-action="Create">
				<div asp-validation-summary="ModelOnly" class="text-danger"></div>
				<div class="form-group">
					<label asp-for="Name" class="control-label"></label>
					<input asp-for="Name" class="form-control" />
					<span asp-validation-for="Name" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="RateNumber" class="control-label"></label>
					<input asp-for="RateNumber" class="form-control" />
					<span asp-validation-for="RateNumber" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="Feedback" class="control-label"></label>
					<input asp-for="Feedback" class="form-control" />
					<span asp-validation-for="Feedback" class="text-danger"></span>
				</div>
				<div class="form-group">
					<input type="submit" value="Create" class="btn btn-primary" />
				</div>
			</form>
		</div>
	</div>
</div>


<div>
	<a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
	await Html.RenderPartialAsync("_ValidationScriptsPartial");
}
}
